<?xml version="1.0"?>
<package >
  <metadata>
    <id>GameCtor.Firebase.Database.Rx</id>
    <version>1.2.0</version>
    <authors>Colt Alan Bauman</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Reactive extension wrappers for Xamarin's Firebase Database package (iOS and Android).</description>
    <projectUrl>https://github.com/cabauman/FirebaseDatabaseRx</projectUrl>
    <releaseNotes>
      - Update System.Reactive to 4.3.2.
      - Update Firebase.Database Android to 71.1601.0.
      - Update Firebase.Database iOS to 6.0.0.1.
    </releaseNotes>
    <copyright>Copyright Â© 2017 Colt Alan Bauman</copyright>
    <dependencies>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="System.Reactive" version="4.3.2" />
        <dependency id="Xamarin.Firebase.iOS.Database" version="6.0.0.1" />
      </group>
      <group targetFramework="MonoAndroid81">
        <dependency id="System.Reactive" version="4.3.2" />
        <dependency id="Xamarin.Firebase.Database" version="71.1601.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
     <!--Xamarin.iOS-->
     <file src="..\src\GameCtor.Firebase.iOS.Database.Rx\bin\Release\GameCtor.Firebase.iOS.Database.Rx.*" target="lib\Xamarin.iOS10" />

     <!--Xamarin.Android-->
     <file src="..\src\GameCtor.Firebase.Android.Database.Rx\bin\Release\GameCtor.Firebase.Android.Database.Rx.*" target="lib\MonoAndroid81" />
  </files>
</package>